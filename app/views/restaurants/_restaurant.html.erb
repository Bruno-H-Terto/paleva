<div><%= restaurant.name %> - <%= restaurant.code %></div>

<table>
  <p>Editar</p>
  <caption>Informações</caption>
  <tbody>
    <tr>
      <td>
        <address><%= restaurant.address.street %>, <%= restaurant.address.number %></address>
        <% if restaurant.address.complement.present? %>
          <address><%= restaurant.address.complement %></address>
        <% end %>
        <address><%= restaurant.address.district %></address>
        <address><%= restaurant.address.city %>, <%= restaurant.address.state %></address>
        <address><%= restaurant.address.zip_code %></address>
      </td>
    </tr>
  </tbody>
</table>
